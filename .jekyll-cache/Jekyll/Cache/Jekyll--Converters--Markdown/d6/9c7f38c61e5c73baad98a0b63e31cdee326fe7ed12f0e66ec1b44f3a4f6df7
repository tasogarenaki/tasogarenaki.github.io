</pre></td> --><td class="rouge-code"><pre><span class="c1"># ------------------------------------- Q5 ------------------------------------- #
</span><span class="k">def</span> <span class="nf">berry_finder</span><span class="p">(</span><span class="n">t</span><span class="p">):</span>
    <span class="s">"""Returns True if t contains a node with the value 'berry' and 
    False otherwise.

    &gt;&gt;&gt; scrat = tree('berry')
    &gt;&gt;&gt; berry_finder(scrat)
    True
    &gt;&gt;&gt; sproul = tree('roots', [tree('branch1', [tree('leaf'), tree('berry')]), tree('branch2')])
    &gt;&gt;&gt; berry_finder(sproul)
    True
    """</span>
    <span class="s">"*** YOUR CODE HERE ***"</span>
    <span class="k">if</span> <span class="n">label</span><span class="p">(</span><span class="n">t</span><span class="p">)</span> <span class="o">==</span> <span class="s">'berry'</span><span class="p">:</span>
        <span class="k">return</span> <span class="bp">True</span>
    <span class="c1"># if branch is empty, return False, cause the label already checked 
</span>    <span class="k">if</span> <span class="n">is_leaf</span><span class="p">(</span><span class="n">t</span><span class="p">):</span>
        <span class="k">return</span> <span class="bp">False</span>
    <span class="c1"># get into every branches
</span>    <span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="n">branches</span><span class="p">(</span><span class="n">t</span><span class="p">):</span>
        <span class="c1"># use recursion to locate all labels every leaves
</span>        <span class="k">if</span> <span class="n">berry_finder</span><span class="p">(</span><span class="n">i</span><span class="p">):</span>
            <span class="k">return</span> <span class="bp">True</span>
    <span class="k">return</span> <span class="bp">False</span>
</pre></td></tr></tbody></table></code></pre></div></div>

