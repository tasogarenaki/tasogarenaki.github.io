</span><span class="k">def</span> <span class="nf">time_per_word</span><span class="p">(</span><span class="n">times_per_player</span><span class="p">,</span> <span class="n">words</span><span class="p">):</span>
    <span class="s">"""Given timing data, return a game data abstraction, which contains a list
    of words and the amount of time each player took to type each word.
    
    &gt;&gt;&gt; p = [[1, 4, 6, 7], [0, 4, 6, 9]]
    &gt;&gt;&gt; words = ['This', 'is', 'fun']
    &gt;&gt;&gt; game = time_per_word(p, words)
    &gt;&gt;&gt; all_words(game)   ----&gt; game[0]
    ['This', 'is', 'fun']
    &gt;&gt;&gt; all_times(game)   ----&gt; game[1]
    [[3,2,1],[4,2,3]]

    &gt;&gt;&gt; p = [[0, 2, 3], [2, 4, 7]]
    &gt;&gt;&gt; game = time_per_word(p, ['hello', 'world'])
    &gt;&gt;&gt; word_at(game, 1)   ----&gt; game[0][word_index]
    'world'
    &gt;&gt;&gt; all_times(game)
    [[2,1],[2,3]]
    &gt;&gt;&gt; time(game, 0, 1)   ----&gt; game[1][player_num][word_index]
    1: 0 -&gt; [2,1] -&gt; 1 -&gt; [1]
    """</span>
    <span class="c1"># BEGIN PROBLEM 9
</span>    <span class="s">"*** YOUR CODE HERE ***"</span>
    <span class="c1"># Initial 2D-Array
</span>    <span class="c1"># times[i][j] with player i (num) and index of word j (num)
</span>    <span class="n">times</span> <span class="o">=</span> <span class="p">[[</span><span class="mi">0</span> <span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">j</span><span class="p">)</span><span class="o">-</span><span class="mi">1</span><span class="p">)]</span> <span class="k">for</span> <span class="n">j</span> <span class="ow">in</span> <span class="n">times_per_player</span><span class="p">]</span>
    <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span>
    <span class="k">for</span> <span class="n">layer1</span> <span class="ow">in</span> <span class="n">times_per_player</span><span class="p">:</span>
        <span class="c1"># first value represents the initial starting time
</span>        <span class="n">j</span> <span class="o">=</span> <span class="mi">0</span>
        <span class="k">for</span> <span class="n">layer0</span> <span class="ow">in</span> <span class="n">layer1</span><span class="p">:</span>
            <span class="k">if</span> <span class="n">j</span> <span class="o">!=</span> <span class="mi">0</span><span class="p">:</span>
                <span class="c1"># e.g. times[0][0] = times_per_player[0][1] - times_per_player[0][0]
</span>                <span class="c1">#                  = 4 - 1 = 3
</span>                <span class="n">times</span><span class="p">[</span><span class="n">i</span><span class="p">][</span><span class="n">j</span><span class="o">-</span><span class="mi">1</span><span class="p">]</span> <span class="o">=</span> <span class="n">layer0</span> <span class="o">-</span> <span class="n">layer1</span><span class="p">[</span><span class="n">j</span><span class="o">-</span><span class="mi">1</span><span class="p">]</span>
            <span class="n">j</span> <span class="o">+=</span> <span class="mi">1</span>
        <span class="n">i</span> <span class="o">+=</span> <span class="mi">1</span>
    <span class="c1"># change the order as rrequested
</span>    <span class="k">return</span> <span class="n">game</span><span class="p">(</span><span class="n">words</span><span class="p">,</span> <span class="n">times</span><span class="p">)</span>
</pre></td></tr></tbody></table></code></pre></div></div>

